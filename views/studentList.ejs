<h1 class="display-4">My Students</h1>
<table class="table table-striped">
  <a class="btn btn-info" href="#" onclick="window.location.reload(true);">Load New Students</a>
  <thead>
    <tr>
      <th>Students Name</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% teacher.students.forEach(function(teacher){ %>
      <tr>
        <td>
          <strong>
            <%= teacher.name %>
          </strong>
        </td>
      </tr>
      <% }) %>
  </tbody>
</table>